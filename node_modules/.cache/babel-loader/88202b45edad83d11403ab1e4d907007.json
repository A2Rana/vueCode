ta = Object.values(await this.getData(`${this.url}/facility=${this.overheadName}/expense/${this.year}`));\n            if (this.data.length) {\n                this.tableHeaders = Object.keys(this.data[0]);\n            } else {\n                this.tableHeaders = ['Data is not present!'];\n            }\n        },\n    },\n    async mounted() {\n        this.data = Object.values(await this.getData(this.url));\n        if (this.data.length) {\n            this.tableHeaders = Object.keys(this.data[0]);\n        } else {\n            this.tableHeaders = ['Data is not present!'];\n        }\n    },\n};\n</script>\n\n<style scoped>\ninput {\n    position: relative;\n    text-transform: capitalize;\n}\n\ntable {\n    display: inline-block;\n    width: 69%;\n}\n</style>\n"],"sourceRoot":""}]}