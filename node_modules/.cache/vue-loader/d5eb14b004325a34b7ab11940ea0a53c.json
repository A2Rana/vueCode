er expenses by Vendor',\n            message:'Enter Vendor Id',\n            data: [],\n            tableHeaders: [],\n            ctcdata: [],\n            id:Number,\n            year:Number,\n            show:false,\n            route:'http://expensetracker.exitest.com/expenseVendor'\n        };\n    },\n    methods: {\n        async getData(vID, year) {\n            this.id=vID.slice(4);\n            this.year=year;\n            const response = await fetch(`https://fbc.exitest.com/vendor/${this.id}/earning/${this.year}`);\n            if (response.ok) {\n                const data = await response.json();\n                this.data = Object.values(data);\n                this.show=true;\n                return data;\n            } else {\n                return console.log('HTTP-Error: ' + response.status);\n            }\n        },\n    },\n    async mounted() {\n        if (this.data.length) {\n            this.tableHeaders = Object.keys(this.data[0]);\n        } else {\n            this.tableHeaders = ['Data is not present!'];\n        }\n    },\n};\n</script>\n\n<style scoped>\n    table{\n        display: inline-block;\n    }\n</style>\n"]}]}